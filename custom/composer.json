{
    "name": "custom/easy-cache",
    "description": "A PSR-6/PSR-16 compatible caching library with support for files, Redis, and Memcached",
    "keywords": ["psr", "psr-16", "cache", "simple-cache", "caching"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Deem Moor",
            "homepage": "https://deemmoor.ru/"
        }
    ],
    "require": {
        "php": ">=8.3",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
        "predis/predis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Custom\\EasyCache\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": {
        "easy-cache": {
            "type": "git",
            "url": "https://github.com/DeemMoor/easy-cache.git"
        }
    }
}
